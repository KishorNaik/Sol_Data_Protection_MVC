
@model List<Sol_DP.Models.UserModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                First Name
            </th>
            <th>
              Last Name
            </th>
            <th>
               Details
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                   @item.FirstName
                </td>
                <td>
                    @item.LastName
                </td>
               
                <td>
                    <a class="btn btn-danger" asp-action="Details" asp-route-id="@item.TokenId">Details</a>
                </td>
            </tr>
        }
    </tbody>
</table>

